<templateSet group="AngularJS DMN">
  <template name="directive" value=" angular.module('tonic.directives.$NAME$', [])&#10;    .directive('$NAME_LOWER$', $NAME$Directive)&#10;    .controller('$NAME$Controller', $NAME$Controller);" description="Directive Template" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="NAME_LOWER" expression="camelCase(NAME)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="route" value="{&#10;      id: '$id$',&#10;      uiStateConfigObject: {&#10;        url: '/$url$',&#10;        templateUrl: tmplPath.spritz + '$tpl$.html',&#10;        controller: '$ctrl$Controller',&#10;        controllerAs: '$ctrlAs$'&#10;      }&#10;    }" description="new route" toReformat="false" toShortenFQNames="true">
    <variable name="id" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="url" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="tpl" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ctrl" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ctrlAs" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="module" value="angular.module('tonic.$TYPE$s.$NAME$', [])&#10;.$TYPE$('$NAME$', $CLASS_NAME$);" description="New AngularJS module" toReformat="false" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CLASS_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="NAME" expression="CLASS_NAME" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>